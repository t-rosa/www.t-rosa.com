---
import { Image } from "astro:assets";
import astradal from "../assets/projects/astradal.png";
import editeurCartographique from "../assets/projects/editeur-cartographique.png";
import janeDoAgency from "../assets/projects/jane-do-agency.png";
import maisonDeLaMascotte from "../assets/projects/maison-de-la-mascotte.png";
import reactWeaver from "../assets/projects/react-weaver.png";
import tanstackForm from "../assets/projects/tanstack-form.png";
import Container from "../components/container.astro";
import Layout from "../layouts/Layout.astro";

const title = "Toma ROSA · Développeur Web Full Stack · Portfolio";
const description =
  "Développeur Full Stack spécialisé en cartographie. Expert React, ASP.NET Core, OpenLayers.";
const keywords =
  "développeur web, full stack, react, asp.net core, openlayers, cartographie, SaaS, toulouse, frontend, backend, javascript, typescript, c#";

const structuredData = {
  "@context": "https://schema.org",
  "@type": "Person",
  name: "Toma ROSA",
  url: "https://www.t-rosa.com",
  description: description,
  image: "https://www.t-rosa.com/og-image.png",
  sameAs: [
    "https://github.com/t-rosa",
    "https://www.linkedin.com/in/toma-rosa/",
    "https://bsky.app/profile/t-rosa.com",
  ],
  jobTitle: "Développeur Web Full Stack",
  location: {
    "@type": "City",
    name: "Toulouse",
  },
  worksFor: {
    "@type": "Organization",
    name: "Astradal",
    url: "https://www.astradal.com",
  },
  knowsAbout: [
    "React",
    "TypeScript",
    "ASP.NET Core",
    "OpenLayers",
    "PostgreSQL",
    "Cartographie",
    "Web Development",
    "Full Stack Development",
  ],
};
---

<Layout
  title={title}
  description={description}
  keywords={keywords}
  structuredData={structuredData}
>
  <Container class="border-b">
    <header class="flex items-center justify-between border-x p-3">
      <h1 class="font-code text-2xl font-bold">
        Toma<span class="text-primary">:</span>Rosa
      </h1>
      <div class="flex items-center gap-3">
        <a
          href="https://github.com/t-rosa"
          target="_blank"
          rel="noopener noreferrer"
          aria-label="Github profile"
        >
          <svg viewBox="0 0 128 128" class="size-6">
            <g fill="white"
              ><path
                fill-rule="evenodd"
                clip-rule="evenodd"
                d="M64 5.103c-33.347 0-60.388 27.035-60.388 60.388 0 26.682 17.303 49.317 41.297 57.303 3.017.56 4.125-1.31 4.125-2.905 0-1.44-.056-6.197-.082-11.243-16.8 3.653-20.345-7.125-20.345-7.125-2.747-6.98-6.705-8.836-6.705-8.836-5.48-3.748.413-3.67.413-3.67 6.063.425 9.257 6.223 9.257 6.223 5.386 9.23 14.127 6.562 17.573 5.02.542-3.903 2.107-6.568 3.834-8.076-13.413-1.525-27.514-6.704-27.514-29.843 0-6.593 2.36-11.98 6.223-16.21-.628-1.52-2.695-7.662.584-15.98 0 0 5.07-1.623 16.61 6.19C53.7 35 58.867 34.327 64 34.304c5.13.023 10.3.694 15.127 2.033 11.526-7.813 16.59-6.19 16.59-6.19 3.287 8.317 1.22 14.46.593 15.98 3.872 4.23 6.215 9.617 6.215 16.21 0 23.194-14.127 28.3-27.574 29.796 2.167 1.874 4.097 5.55 4.097 11.183 0 8.08-.07 14.583-.07 16.572 0 1.607 1.088 3.49 4.148 2.897 23.98-7.994 41.263-30.622 41.263-57.294C124.388 32.14 97.35 5.104 64 5.104z"
              ></path><path
                d="M26.484 91.806c-.133.3-.605.39-1.035.185-.44-.196-.685-.605-.543-.906.13-.31.603-.395 1.04-.188.44.197.69.61.537.91zm2.446 2.729c-.287.267-.85.143-1.232-.28-.396-.42-.47-.983-.177-1.254.298-.266.844-.14 1.24.28.394.426.472.984.17 1.255zM31.312 98.012c-.37.258-.976.017-1.35-.52-.37-.538-.37-1.183.01-1.44.373-.258.97-.025 1.35.507.368.545.368 1.19-.01 1.452zm3.261 3.361c-.33.365-1.036.267-1.552-.23-.527-.487-.674-1.18-.343-1.544.336-.366 1.045-.264 1.564.23.527.486.686 1.18.333 1.543zm4.5 1.951c-.147.473-.825.688-1.51.486-.683-.207-1.13-.76-.99-1.238.14-.477.823-.7 1.512-.485.683.206 1.13.756.988 1.237zm4.943.361c.017.498-.563.91-1.28.92-.723.017-1.308-.387-1.315-.877 0-.503.568-.91 1.29-.924.717-.013 1.306.387 1.306.88zm4.598-.782c.086.485-.413.984-1.126 1.117-.7.13-1.35-.172-1.44-.653-.086-.498.422-.997 1.122-1.126.714-.123 1.354.17 1.444.663zm0 0"
              ></path></g
            >
          </svg>
        </a>

        <a
          href="https://www.linkedin.com/in/toma-rosa/"
          target="_blank"
          rel="noopener noreferrer"
          aria-label="Linkedin profile"
        >
          <svg viewBox="0 0 128 128" class="size-6">
            <path
              fill="#0076b2"
              d="M116 3H12a8.91 8.91 0 00-9 8.8v104.42a8.91 8.91 0 009 8.78h104a8.93 8.93 0 009-8.81V11.77A8.93 8.93 0 00116 3z"
            ></path><path
              fill="#fff"
              d="M21.06 48.73h18.11V107H21.06zm9.06-29a10.5 10.5 0 11-10.5 10.49 10.5 10.5 0 0110.5-10.49M50.53 48.73h17.36v8h.24c2.42-4.58 8.32-9.41 17.13-9.41C103.6 47.28 107 59.35 107 75v32H88.89V78.65c0-6.75-.12-15.44-9.41-15.44s-10.87 7.36-10.87 15V107H50.53z"
            ></path>
          </svg>
        </a>
      </div>
    </header>
  </Container>
  <Container class="border-b">
    <section class="grid-background grid place-items-center border-x p-6">
      <div>
        <span class="font-code text-lg"
          ><span class="text-primary">[</span>Web<span class="text-primary"
            >]</span
          ></span
        >
        <h2 class="text-center text-4xl font-bold">Developper</h2>
      </div>
    </section>
  </Container>
  <Container class="border-b">
    <section id="projects" class="border-x">
      <div class="border-b">
        <h2 class="font-code p-6 text-center text-3xl sm:text-left">
          Projects<span class="text-primary">[]</span>
        </h2>
      </div>
      <div class="grid sm:grid-cols-2">
        <a
          href="https://app.astradal.com/home/"
          target="_blank"
          rel="noopener noreferrer"
          class="hover:bg-primary/10 group cursor-pointer border-b duration-150 sm:border-r"
        >
          <div class="flex items-center gap-3 border-b pr-3">
            <code class="border-r p-2.5">
              <span class="text-primary">[</span>0<span class="text-primary"
                >]</span
              >
            </code>
            <h3 class="font-bold">Editeur cartographique</h3>
          </div>
          <div class="p-3">
            <Image
              width="1280"
              height="720"
              priority
              src={editeurCartographique}
              alt="Éditeur cartographique"
              class="group-hover:border-primary/30 border-2 duration-150"
            />
          </div>
        </a>

        <a
          href="https://www.astradal.com"
          target="_blank"
          rel="noopener noreferrer"
          class="hover:bg-primary/10 group cursor-pointer border-b duration-150"
        >
          <div class="flex items-center gap-3 border-b pr-3">
            <code class="border-r p-2.5">
              <span class="text-primary">[</span>1<span class="text-primary"
                >]</span
              >
            </code>
            <h3 class="font-bold">Astradal</h3>
          </div>
          <div class="p-3">
            <Image
              width="1280"
              height="720"
              src={astradal}
              alt="Astradal"
              class="group-hover:border-primary/30 border-2 duration-150"
            />
          </div>
        </a>

        <a
          href="https://www-janedoagency-com.vercel.app/"
          target="_blank"
          rel="noopener noreferrer"
          class="hover:bg-primary/10 group cursor-pointer border-b duration-150 sm:border-r"
        >
          <div class="flex items-center gap-3 border-b pr-3">
            <code class="border-r p-2.5">
              <span class="text-primary">[</span>4<span class="text-primary"
                >]</span
              >
            </code>
            <h3 class="font-bold">Jane Do Agency</h3>
          </div>
          <div class="p-3">
            <Image
              width="1280"
              height="720"
              priority
              src={janeDoAgency}
              alt="Jane Do Agency"
              class="group-hover:border-primary/30 border-2 duration-150"
            />
          </div>
        </a>

        <a
          href="https://github.com/t-rosa/ReactWeaver/"
          target="_blank"
          rel="noopener noreferrer"
          class="hover:bg-primary/10 group group cursor-pointer border-b duration-150"
        >
          <div class="flex items-center gap-3 border-b pr-3">
            <code class="border-r p-2.5">
              <span class="text-primary">[</span>3<span class="text-primary"
                >]</span
              >
            </code>
            <h3 class="font-bold">React Weaver</h3>
          </div>
          <div class="p-3">
            <Image
              width="1280"
              height="720"
              src={reactWeaver}
              alt="React Weaver"
              class="group-hover:border-primary/30 border-2 duration-150"
            />
          </div>
        </a>

        <a
          href="https://github.com/TanStack/form/pulls?q=is%3Apr+author%3At-rosa+"
          target="_blank"
          rel="noopener noreferrer"
          class="hover:bg-primary/10 group cursor-pointer border-b duration-150 sm:border-r sm:border-b-0"
        >
          <div class="flex items-center gap-3 border-b pr-3">
            <code class="border-r p-2.5">
              <span class="text-primary">[</span>2<span class="text-primary"
                >]</span
              >
            </code>
            <h3 class="font-bold">Tanstack Form</h3>
          </div>
          <div class="p-3">
            <Image
              width="1280"
              height="720"
              src={tanstackForm}
              alt="Tanstack Form"
              class="group-hover:border-primary/30 border-2 duration-150"
            />
          </div>
        </a>

        <a
          href="https://www.maisondelamascotte.com/"
          target="_blank"
          rel="noopener noreferrer"
          class="hover:bg-primary/10 group group cursor-pointer duration-150"
        >
          <div class="flex items-center gap-3 border-b pr-3">
            <code class="border-r p-2.5">
              <span class="text-primary">[</span>5<span class="text-primary"
                >]</span
              >
            </code>
            <h3 class="font-bold">Maison de la mascotte</h3>
          </div>
          <div class="p-3">
            <Image
              width="1280"
              height="720"
              src={maisonDeLaMascotte}
              alt="Maison de la mascotte"
              class="group-hover:border-primary/30 border-2 duration-150"
            />
          </div>
        </a>
      </div>
    </section>
  </Container>
</Layout>
