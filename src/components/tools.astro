---

<Container class="border-b" }>
  <section id="tools">
    <div class="border-x">
      <h2 class="font-code p-6 text-center text-3xl sm:text-left">
        Tools<span class="text-primary">[]</span>
      </h2>
    </div>

    <div class="grid grid-cols-2 md:grid-cols-4">
      {
        tools.map((tool, index) => (
          <article class="hover:bg-primary/10 group border border-r-0 border-b-0 duration-150 nth-[2n]:border-r md:nth-[2n]:border-r-0 md:nth-[4n]:border-r">
            <div class="flex items-center gap-1.5 border-b pr-1.5 sm:gap-3 sm:pr-3">
              <code class="flex border-r p-1.5 sm:p-2.5">
                <span class="text-primary">[</span>
                {index}
                <span class="text-primary">]</span>
              </code>
              <h3 class="text-xs font-bold sm:text-base">{tool.title}</h3>
            </div>
            <div
              class="grid h-32 place-items-center [&>svg]:size-16"
              set:html={tool.icon}
            />
          </article>
        ))
      }
    </div>
  </section>
</Container>
